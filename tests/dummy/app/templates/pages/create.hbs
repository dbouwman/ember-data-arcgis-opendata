<h2>Create Page </h2>

<div class="row">
  <div class="col-md-12" id="jsoneditor-container">

    {{json-editor-for model mode='code'  }}
    <br>
    <button type="button" name="button" class="btn btn-primary"
        {{action 'save' model}}>Save</button>
    <button type="button" name="button" class="btn btn-default"
        {{action 'cancel'}}>Cancel</button>
  </div>
</div>
